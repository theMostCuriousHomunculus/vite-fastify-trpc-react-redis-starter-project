{
  "name": "vite-fastify-trpc-react-redis-starter-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build --outDir ../../dist/client --ssrManifest",
    "build:server": "vite build --outDir ../../dist/server --ssr ../server/index.ts",
    "dev": "nodemon src/server/index.ts --dev",
    "docker-dev-down": "docker compose -f docker-compose.development.yml down",
    "docker-dev-up": "docker compose -f docker-compose.development.yml --env-file .env.development up -d",
    "lint": "eslint ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/env": "^4.2.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/vite": "^4.0.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "@react-google-maps/api": "^2.18.1",
    "@tanstack/react-query": "^4.29.14",
    "@tanstack/router": "0.0.1-beta.84",
    "@trpc/client": "^10.31.0",
    "@trpc/react-query": "^10.31.0",
    "@trpc/server": "^10.31.0",
    "fastify": "^4.18.0",
    "fastify-socket.io": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@types/google.maps": "^3.53.4",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "env-schema": "^5.2.0",
    "eslint": "^8.38.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "zod": "^3.21.4"
  }
}
